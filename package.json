{
    "name": "axion",
    "version": "0.1.0",
    "description": "Boilerplate",
    "exports": {
        "./*": "./*.js"
    },
    "type": "module",
    "scripts": {
        "start": "node index.js",
        "dev": "NODE_ENV=development nodemon --require dotenv/config  index.js dotenv_config_path=.env.development",
        "test": "node --experimental-vm-modules --no-warnings  node_modules/jest/bin/jest.js",
        "seed": "NODE_ENV=development node --require dotenv/config  seed.development.js dotenv_config_path=.env.development",
        "prepare": "husky install",
        "lint": "eslint . --fix",
        "release": "semantic-release"
    },
    "author": "",
    "license": "ISC",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "dependencies": {
        "@elastic/ecs-winston-format": "^1.5.3",
        "bcrypt": "^5.1.1",
        "bull": "^4.16.5",
        "cors": "^2.8.5",
        "debug": "^4.4.0",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-list-endpoints": "^7.1.1",
        "glob": "^11.0.1",
        "ion-cortex": "^2.1.5",
        "ioredis": "^5.4.2",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "md5": "^2.3.0",
        "mongoose": "^8.9.5",
        "nanoid": "^5.0.9",
        "oyster-db": "*",
        "qantra-pineapple": "^1.1.8",
        "request-ip": "^3.3.0",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "tweetnacl": "^1.0.3",
        "tweetnacl-util": "^0.15.1",
        "useragent": "^2.3.0",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.6.1",
        "@commitlint/config-conventional": "^19.6.0",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "^13.0.1",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^11.0.1",
        "@semantic-release/release-notes-generator": "^14.0.3",
        "@types/jest": "^29.5.14",
        "commitizen": "^4.3.1",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.57.1",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "husky": "^8.0.0",
        "jest": "^29.7.0",
        "lint-staged": "^15.3.0",
        "nodemon": "^3.1.9",
        "prettier": "^3.4.2",
        "semantic-release": "^24.2.1",
        "supertest": "^7.0.0"
    }
}
